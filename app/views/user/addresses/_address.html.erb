<div id="<%= dom_id address %>">
  <p>
    <strong>Address Type:</strong>
    <%= address.address_type %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= address.address %>
  </p>

  <p>
    <strong>City:</strong>
    <%= address.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= address.state %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= address.country %>
  </p>

  <p>
    <strong>Zipcode:</strong>
    <%= address.zipcode %>
  </p>

  <p>
    <strong>Is active:</strong>
    <%= address.is_active %>
  </p>

</div>
